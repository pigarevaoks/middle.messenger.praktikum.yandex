{
    "tabWidth": 4,
    "useTabs": false,
    "printWidth": 100,
    "semi": false,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "jsxSingleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": false,
    "jsxBracketSameLine": false,
    "arrowParens": "always",
    "endOfLine": "auto",
    "overrides": [
        {
            "files": [
                "*.tmpl.ts"
            ],
            "options": {
                "parser": "go-template",
                "goTemplateBracketSpacing": true
            }
        }
    ]
}
